<td id="mwAqA">20</td><td id="mwAqE">Eudicot</td><td id="mwAqI">Acanthaceae</td><td id="mwAqQ">acanthus</td><td id="mwAqY">3,500</td></tr>
<tr id="mwAqc">
<td id="mwAqg">21</td><td id="mwAqk">Eudicot</td><td id="mwAqo">Rosaceae</td><td id="mwAqw">rose</td><td id="mwAq4">2,830</td></tr>
<tr id="mwAq8">
<td id="mwArA">22</td><td id="mwArE">Eudicot</td><td id="mwArI">Boraginaceae</td><td id="mwArQ">borage</td><td id="mwArY">2,740</td></tr>
<tr id="mwArc">
<td id="mwArg">23</td><td id="mwArk">Eudicot</td><td id="mwAro">Urticaceae</td><td id="mwArw">nettle</td><td id="mwAr4">2,625</td></tr>
<tr id="mwAr8">
