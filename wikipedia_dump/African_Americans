<th colspan="2" id="mwBJk">Black Hispanics</th>
<th rowspan="2" id="mwBJs">Total</th></tr>
<tr id="mwBJw">
<th id="mwBJ0">#</th>
<th id="mwBJ4"><span typeof="mw:DisplaySpace" id="mwBJ8">&nbsp;</span>%</th>
<th id="mwBKA">#</th>
<th id="mwBKE"><span typeof="mw:DisplaySpace" id="mwBKI">&nbsp;</span>%</th></tr>
<tr id="mwBKM">
<td id="mwBKQ">2020</td>
<td id="mwBKU">39,940,338</td>
<td id="mwBKY">12.1%</td>
<td id="mwBKc">1,163,862</td>
