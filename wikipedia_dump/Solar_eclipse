<td rowspan="6" id="mwBPQ"><span typeof="mw:Entity" id="mwBPU">&nbsp;</span></td>
<th scope="col" colspan="3" id="mwBPY">Descending node</th></tr>
<tr style="text-align: center;" id="mwBPc">
<th scope="col" id="mwBPg">Saros</th>
<th scope="col" id="mwBPk">Map</th>
<th scope="col" id="mwBPo">Gamma</th>
<th scope="col" id="mwBPs">Saros</th>
<th scope="col" id="mwBPw">Map</th>
<th scope="col" id="mwBP0">Gamma</th></tr>
<tr style="text-align: center;" id="mwBP4">
<td id="mwBP8">119<br id="mwBQA"><span typeof="mw:File" id="mwBQE"><img src="./lossy-page1-150px-30_April_2022_Partial_Solar_Eclipse_(CTIO_20220430_Eclipse-Solar-Partial_DMunizaga-CC).tiff.jpg" decoding="async" data-file-width="3955" data-file-height="3303" data-file-type="bitmap" height="125" width="150" class="mw-file-element" id="mwBQM" loading="lazy"></span><br id="mwBQQ">Partial in CTIO, Chile</td>
<td id="mwBQc">April 30, 2022<br id="mwBQk"><span typeof="mw:File" id="mwBQo"><img src="./SE2022Apr30P.png" decoding="async" data-file-width="728" data-file-height="729" data-file-type="bitmap" height="150" width="150" class="mw-file-element" id="mwBQw" loading="lazy"></span><br id="mwBQ0">Partial</td>
