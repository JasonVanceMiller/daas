<tr id="mwBo0">
<td id="mwBo4">2013</td><td id="mwBo8">22,644</td><td id="mwBpA"><span typeof="mw:Transclusion mw:File" id="mwBpE"><span title="Increase"><img alt="Increase" src="./Increase2.svg.png" decoding="async" data-file-width="300" data-file-height="300" data-file-type="drawing" height="11" width="11" class="mw-file-element" loading="lazy"></span></span><span aria-hidden="true" style="visibility:hidden;color:transparent;" typeof="mw:Transclusion" id="mwBpI">0</span>4.1%</td></tr>
